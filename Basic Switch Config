!Basic Cisco Router Config

hostname Router-A
enable secret 5 SuperSecret
enable password SuperSecret
line vty 0 4
password SuperSecret
!
interface Vlan1
 shutdown
 no ip address
!
interface GigabitEthernet0/0
 no shutdown
 ip address 192.168.2.1 255.255.255.0
!
interface GigabitEthernet0/1
 shutdown
 no ip address
!
interface GigabitEthernet0/2
 shutdown
 no ip address
!
end
